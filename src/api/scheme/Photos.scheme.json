{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Photos",
  "definitions": {
    "Photos": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "sol": {
            "type": "number"
          },
          "camera": {
            "type": "object",
            "properties": {
              "id": {
                "type": "number"
              },
              "name": {
                "type": "string"
              },
              "rover_id": {
                "type": "number"
              },
              "full_name": {
                "type": "string"
              }
            },
            "required": ["id", "name", "rover_id", "full_name"],
            "additionalProperties": false
          },
          "img_src": {
            "type": "string"
          },
          "earth_date": {
            "type": "string"
          },
          "rover": {

              "type": "object",
              "properties": {
                "id": {
                  "type": "number"
                },
                "name": {
                  "type": "string"
                },
                "landing_date": {
                  "type": "string"
                },
                "launch_date": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                }
              },
              "required": ["id", "name", "landing_date", "launch_date", "status"],
              "additionalProperties": false
          }
        },
        "required": ["id", "sol", "camera", "img_src", "earth_date", "rover"],
        "additionalProperties": false
      }
    }
  }
}
